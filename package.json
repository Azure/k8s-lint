{
   "name": "k8s-lint-action",
   "version": "0.0.0",
   "private": true,
   "main": "lib/run.js",
   "scripts": {
      "prebuild": "npm i @vercel/ncc",
      "build": "ncc build src/run.ts -o lib",
      "test": "jest",
      "test-coverage": "jest --coverage",
      "format": "prettier --write .",
      "format-check": "prettier --check ."
   },
   "keywords": [
      "actions"
   ],
   "author": "GitHub",
   "license": "MIT",
   "dependencies": {
      "@actions/core": "^1.10.0",
      "@actions/exec": "^1.0.0",
      "@actions/tool-cache": "1.1.2"
   },
   "devDependencies": {
      "@types/jest": "^26.0.0",
      "@types/node": "^12.20.41",
      "@vercel/ncc": "^0.34.0",
      "jest": "^29.7.0",
      "prettier": "^2.8.8",
      "ts-jest": "^29.2.3",
      "typescript": "5.5.4"
   }
}
